# coding: utf-8
from pwn import *
from Crypto.Util.number import *

with open('ct') as f:
    ct = f.read().strip()
with open('flags.txt') as f:
    flags = [l.strip() for l in f]

the_flag = ''
size = 32

for i in range(0, len(ct), size):
    chunk = ct[i:i+size]
    print '\n' + chunk
    print '-'*100
    for line in flags:
        line = line.split(':')
        flag = line[0]
        h_ct = line[1]
        if chunk in h_ct:
            h_index = h_ct.find(chunk)
            f_index = h_index / 2
            print h_ct[h_index:h_index+size]# h_ct[i:i+size]
            print flag[f_index:f_index+(size/2)] # flag[(i/2):(i/2)+(size/2)]
            print flag
            print h_ct
            the_flag += flag[f_index:f_index+(size/2)]
            break
print '\n'
print '*'*100
print ct
print the_flag
    
